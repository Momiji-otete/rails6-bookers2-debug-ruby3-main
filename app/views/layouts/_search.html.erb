<div class="container">
  <%= form_with url: search_path, local: true, method: :get do |f| %>
    <%= f.search_field :search_word %>
    <%= f.select :model_name, {"User"=>"user", "Book"=>"book"} %>
    <%= f.select :method, {"完全一致"=>"perfect", "前方一致"=>"forward", "後方一致"=>"backward", "部分一致"=>"partial" } %>
    <%= f.submit "検索" %>
  <% end %>
</div>
